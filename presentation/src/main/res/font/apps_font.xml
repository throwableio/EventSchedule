<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <!-- light -->
    <font
        android:font="@font/saileclight"
        android:fontStyle="normal"
        android:fontWeight="200"

        app:font="@font/saileclight"
        app:fontStyle="normal"
        app:fontWeight="200" />

    <!-- italic -->
    <!-- regular -->
    <font
        android:font="@font/sailecregular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/sailecregular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- italic -->
    <font
        android:font="@font/sailecmedium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/sailecmedium"
        app:fontStyle="italic"
        app:fontWeight="500" />

    <!-- bold -->
    <font
        android:font="@font/sailecbold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/sailecbold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>